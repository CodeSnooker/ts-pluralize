    "@types/jest": "^23.3.2",
    "@types/pluralize": "0.0.29",
    "codecov": "^3.1.0",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
